[General]
network = ZigBee
eventlog-message-detail-pattern = *:name
record-eventlog = false
seed-set = ${runnumber}
sim-time-limit = 36000s
rng-class="cLCG32" #//selected random number generator from omnet++
num-rngs=2 #//for setting explicit seeds to random number generators
seed-0-lcg32 = 9927461
seed-1-lcg32 = 2794954


**.module-eventlog-recording = false
*.playgroundSizeX = 300
*.playgroundSizeY = 300
*.numRouters = 1
